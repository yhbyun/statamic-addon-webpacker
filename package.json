{
  "name": "statamic-addon-webpacker",
  "version": "1.1.8",
  "description": "The power of Webpack inside Statamic",
  "author": "jimblue",
  "license": "MIT",
  "repository": "github:jimblue/statamic-addon-webpacker.git",
  "bugs": "https://github.com/jimblue/statamic-addon-webpacker/issues",
  "homepage": "https://github.com/jimblue/statamic-addon-webpacker#readme",
  "main": "src/index.js",
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "setup": "npm install",
    "reset": "npm run clear:modules && npm run setup",
    "clear:modules": "rm -rf node_modules package-lock.json",
    "clear:cache": "rm -rf node_modules/.cache/*",
    "update:prod": "ncu -u -a -p",
    "update:dev": "ncu -u -a -d",
    "update:boilerplate": "ncu -u -a --packageFile boilerplate/package.json"
  },
  "devDependencies": {
    "eslint": "5.9.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-standard": "4.0.0",
    "npm-check-updates": "2.15.0"
  },
  "dependencies": {
    "@babel/core": "7.2.0",
    "babel-loader": "8.0.4",
    "browser-sync": "2.26",
    "chalk": "2.4.1",
    "clean-webpack-plugin": "1.0.0",
    "cross-env": "5.2.0",
    "css-loader": "2.0.0",
    "eslint": "5.9.0",
    "eslint-loader": "2.1.1",
    "fibers": "3.1.1",
    "file-loader": "2.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "image-webpack-loader": "4.6.0",
    "js-yaml": "3.12.0",
    "lodash": "4.17.11",
    "mini-css-extract-plugin": "0.5.0",
    "node-notifier": "5.3.0",
    "node-sass": "4.10.0",
    "opn": "5.4.0",
    "ora": "3.0.0",
    "postcss-loader": "3.0.0",
    "query-string": "6.2.0",
    "resolve-url-loader": "3.0.0",
    "sass": "1.15.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "9.9.0",
    "stylelint-webpack-plugin": "0.10.5",
    "terser-webpack-plugin": "1.1.0",
    "webpack": "4.27.1",
    "webpack-assets-manifest": "3.1.1",
    "webpack-build-notifier": "0.1.30",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3"
  }
}
