{
  "root": true,
  "extends": ["standard", "plugin:import/errors", "plugin:import/warnings", "plugin:promise/recommended"],
  "plugins": ["import", "promise"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "codeFrame": false,
    "ecmaVersion": 6
  },
  "env": {
    "browser": true
  },
  "globals": {
    "STATAMIC_DEV": true,
    "STATAMIC_PROD": true
  }
}
