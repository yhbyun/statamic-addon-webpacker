{
  "private": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "setup": "npm install",
    "reset": "npm run clear:modules && npm run setup",
    "clear:modules": "rm -rf node_modules package-lock.json",
    "clear:cache": "rm -rf node_modules/.cache/*",
    "webpacker:dev": "node webpack.config.js --mode=development",
    "webpacker:prod": "node webpack.config.js --mode=production"
  },
  "dependencies": {
    "@babel/runtime": "7.1.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/plugin-transform-spread": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "babel-eslint": "10.0.1",
    "css-mqpacker": "7.0.0",
    "cssnano": "v4.1.7",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "postcss-preset-env": "6.4.0",
    "sort-css-media-queries": "1.4.1",
    "statamic-addon-webpacker": "latest",
    "stylelint-config-recess-order": "2.0.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "2.0.0",
    "stylelint-scss": "3.4.0"
  }
}
